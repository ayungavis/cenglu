{
  "name": "cenglu",
  "description": "Fast, zero-dependencies, secure logger for Node.js that doesn't suck. Built for production, not resumes.",
  "version": "2.1.1",
  "type": "module",
  "license": "MIT",
  "author": "Wahyu Kurniawan <ayungavis@gmail.com>",
  "homepage": "https://github.com/ayungavis/cenglu#readme",
  "bugs": {
    "url": "https://github.com/ayungavis/cenglu/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayungavis/cenglu.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./context": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./testing": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./middleware": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./transports": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./format": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./redaction": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "bun x ultracite@latest check",
    "format": "bun x ultracite@latest fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "docs": "typedoc",
    "benchmark": "tsx benchmarks/run.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^20.14.9",
    "@vitest/coverage-v8": "^4.0.15",
    "bunup": "^0.16.10",
    "lefthook": "^2.0.4",
    "size-limit": "^11.2.0",
    "typedoc": "^0.25.13",
    "typescript": "^5.5.4",
    "ultracite": "6.3.8",
    "vitest": "^4.0.15"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "500 KB"
    }
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "logger",
    "logging",
    "log",
    "structured-logging",
    "json-logging",
    "zero-dependency",
    "typescript",
    "nodejs",
    "production-ready",
    "async-local-storage",
    "correlation-id",
    "request-logging",
    "ecs",
    "datadog",
    "splunk",
    "pino",
    "winston",
    "bunyan"
  ]
}
